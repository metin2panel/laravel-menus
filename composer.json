{
  "name": "depiedra/laravel-menus",
  "description": "Laravel Menu management",
  "keywords": [
    "laravel",
    "menus",
    "navigation",
    "nav",
    "bootstrap"
  ],
  "license": "proprietary",
  "authors": [
    {
      "name": "Erdem Aksu",
      "email": "eaksu94@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "laravel/framework": "^8.0",
    "laravelcollective/html": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Depiedra\\Menus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Depiedra\\Menus\\Tests\\": "Tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Depiedra\\Menus\\MenusServiceProvider"
      ],
      "aliases": {
        "Menu": "Depiedra\\Menus\\Facades\\Menu"
      }
    }
  }
}
